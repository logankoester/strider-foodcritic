  "description": "Foodcritic is a helpful lint tool you can use to check your Chef cookbooks for common problems.",
  "main": "webapp.js",
  "scripts": {
    "test": "mocha -R spec --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logankoester/strider-foodcritic.git"
  },
  "keywords": [
    "strider",
    "plugin",
    "chef",
    "linter",
    "foodcritic"
  ],
  "author": "Logan Koester",
  "license": "MIT",
  "dependencies": {},
  "strider": {
